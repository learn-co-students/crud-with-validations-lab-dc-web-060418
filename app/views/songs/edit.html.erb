<h1>Edit Song</h1>

<%= form_for @song do |f| %>
  <%= f.label :Title %><br>
  <%= f.text_field :title %><br>
  <br>
  <%= f.label :Released? %><br>
  <%= f.check_box :released %><br>
  <br>
  <%= f.label :'Release Year' %><br>
  <%= f.text_field :release_year %><br>
  <br>
  <%= f.label :Artist %><br>
  <%= f.text_field :artist_name %></br>
  <br>
  <%= f.label :Genre %><br>
  <%= f.text_field :genre %><br>
  <br>
  <%= f.submit :'Save Song' %>
<% end %>
